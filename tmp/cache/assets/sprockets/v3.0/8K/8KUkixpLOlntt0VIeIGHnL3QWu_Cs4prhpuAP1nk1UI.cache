I"ÿ(function() {
  var admin_search_users;

  admin_search_users = function() {
    return $("#users_searchbar").keyup(function() {
      var a, filter, i, item, len, ref, ul;
      filter = $('#users_searchbar').val().toUpperCase();
      console.log("filter:" + filter);
      ul = $("#admin_users_list");
      ref = $(ul).children();
      for (i = 0, len = ref.length; i < len; i++) {
        item = ref[i];
        a = $(item).children("a");
        console.log("  a.text:" + ($(a).text()));
        if ($(a).text().toUpperCase().indexOf(filter) > -1) {
          $(item).css("display", "");
        } else {
          $(item).css("display", "none");
        }
      }
    });
  };

  $(document).ready(function() {
    admin_search_users();
  });

}).call(this);
