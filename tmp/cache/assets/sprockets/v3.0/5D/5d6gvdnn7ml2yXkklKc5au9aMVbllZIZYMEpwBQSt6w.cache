(function() {
  $(document).ready(function() {
    $("[name='close_chat']").click(function() {
      $(this).parents(".chat_div").css("visibility", "hidden");
    });
    $(".chat_list_item").click(function() {
      var id;
      id = $(this).children("[name='chat_id']").attr("content");
      console.log("chat id: " + id);
    });
  });

}).call(this);
