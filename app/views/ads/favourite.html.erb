<h3> Favourite List </h3>
<% @favourities.each do |book| %>
    <%= image_tag(book.link_to_coverbook) %>
    <%= book.book_title %>
    <%= book.book_authors.tr('[', '').tr(']','').tr('"', '') %>
    <%= book.publisher %>
    <%= book.identifier.tr('[', '').tr(']','').tr('"', '').tr(',','') %>
    <br>
<% end %>